	<definition name="welcomePage" extends="tiles.index.private">
		<put-attribute name="contenidoCentro" value="/WEB-INF/private/welcomePage.jsp" />
	</definition>
	
	<definition name="loginPage" extends="tiles.index.public">
		<put-attribute name="contenidoCentro" value="/WEB-INF/public/loginPage.jsp" />
	</definition>
	
	
	
</tiles-definitions>